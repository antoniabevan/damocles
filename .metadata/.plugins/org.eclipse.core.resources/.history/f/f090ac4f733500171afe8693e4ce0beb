module mcmc

    implicit none

    type mcmc_param_space
        integer :: n_points
    end type

    type(mcmc_param_space) param_space

contains

    subroutine mcmc_sampler()

        open(53,file='input/MCMC.in')
        read(53,*)
        read(53,*) param_space%n_points



    end subroutine

end module mcmc
